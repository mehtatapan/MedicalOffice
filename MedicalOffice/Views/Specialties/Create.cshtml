@model MedicalOffice.Models.Specialty

@{
    ViewData["Title"] = "Create Specialty";
}

<h2>Create Specialty</h2>

<hr />
<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label asp-for="SpecialtyName" class="control-label"></label>
                <input asp-for="SpecialtyName" class="form-control" />
                <span asp-validation-for="SpecialtyName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </div>
    </div>
</form>
<div>
    <a asp-action="Index" asp-controller="Lookups" asp-route-Tab="SpecialtiesTab">Back to Lookup List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
